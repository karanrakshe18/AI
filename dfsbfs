g={
    0:[1,2],
    1:[4,5],
    2:[3],
    3:[2],
    4:[1],
    5:[1]
}
def dfs(g,s):
    vist[s]=1
    print(s)
    for c in g[s]:
        if not vist[c]:
            dfs(g,c)
vist=[0]*6
dfs(g,0)

def bfs(g,s):
    q=[s]
    vis=[s]
    while q:
        cur=q.pop(0)
        print(cur)
        for c in g[cur]:
            if c not in vis:
                q.append(c)
                vis.append(c)
                
bfs(g,0)
